# Bevy credentials (required - add your own)
BEVY_EMAIL=your-email@example.com
BEVY_PASSWORD=your-password

# Bevy event URL (attendees page or base event URL)
BEVY_EVENT_URL=

# Playwright
PW_HEADLESS=true
PW_SLOW_MO_MS=0
AUTH_STATE_PATH=.auth/bevy-auth.json

# Google Sheets
GOOGLE_SHEET_URL=

# Throttling
MIN_DELAY_MS=500
MAX_DELAY_MS=1000
BATCH_SIZE=50
RETRY_LIMIT=2
